<?xml version='1.0'?>
<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns="http://www.w3.org/1999/xhtml" version="1.0">
	<xsl:import href="${docbookXSLPath}/docbook-xsl/xhtml5/chunk.xsl"/>
	<xsl:template name="user.head.content">
		<xsl:variable name="codefile" select="document('${CMAKE_CURRENT_SOURCE_DIR}/VuoManualHeader.xhtml',/)"/>
		<xsl:copy-of select="$codefile/head/node()"/>
	</xsl:template>
	<xsl:template name="user.header.navigation">
		<xsl:variable name="codefile" select="document('${CMAKE_CURRENT_SOURCE_DIR}/VuoManualNavigation.xhtml',/)"/>
		<xsl:copy-of select="$codefile/body/node()"/>
	</xsl:template>
	<xsl:template name="user.footer.content">
		<xsl:variable name="codefile" select="document('${CMAKE_CURRENT_SOURCE_DIR}/VuoManualFooter.xhtml',/)"/>
		<xsl:copy-of select="$codefile/body/node()"/>
	</xsl:template>
	<xsl:template name="gentext.nav.prev">
		<xsl:value-of select="'❮'"/>
	</xsl:template>
	<xsl:template name="gentext.nav.next">
		<xsl:value-of select="'❯'"/>
	</xsl:template>
</xsl:stylesheet>
